@PACKAGE_INIT@

IF(NOT (Git_FOUND OR GIT_FOUND))
	UNSET(@PROJECT_NAME@_FindGit)
	IF(@PROJECT_NAME@_FIND_REQUIRED)
		LIST(APPEND @PROJECT_NAME@_FindGit REQUIRED)
	ENDIF()
	IF(@PROJECT_NAME@_FIND_QUIETLY)
		LIST(APPEND @PROJECT_NAME@_FindGit QUIET)
	ENDIF()
	FIND_PACKAGE(Git ${@PROJECT_NAME@_FindGit})
	IF(NOT (Git_FOUND OR GIT_FOUND))
		SET(@PROJECT_NAME@_FOUND FALSE)
		RETURN()
	ENDIF()
ENDIF()

INCLUDE("${CMAKE_CURRENT_LIST_DIR}/cmake-version4git.cmake")

MESSAGE(STATUS "Found @PROJECT_NAME@: ${CMAKE_CURRENT_LIST_DIR}"
	" (found version \"${@PROJECT_NAME@_VERSION}\")")
